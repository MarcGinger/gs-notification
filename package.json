{
  "name": "gs-scaffold",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm:ts": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "migration:create": "npm run typeorm:ts -- migration:create src/migrations/manual",
    "migration:generate": "npm run typeorm:ts -- migration:generate src/migrations/auto -d src/database/app.datasource.ts",
    "migration:run": "ts-node -r tsconfig-paths/register src/migrations/run.ts",
    "migration:revert": "npm run typeorm:ts -- migration:revert -d src/database/app.datasource.ts",
    "migrate": "npm run migration:run",
    "migrate:product-config": "npm run migration:run -- --module product-config",
    "migrate:dry-run": "npm run migration:run -- --dry-run",
    "migrate:verbose": "npm run migration:run -- --verbose",
    "migrate:status": "ts-node -r tsconfig-paths/register src/migrations/status.ts",
    "doppler": "doppler run --project gs-scaffold-api --config dev_main -- npm run start:dev"
  },
  "dependencies": {
    "@dopplerhq/node-sdk": "^1.3.0",
    "@kurrent/kurrentdb-client": "^1.0.4",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/cqrs": "^11.0.3",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/typeorm": "^11.0.0",
    "@types/helmet": "^4.0.0",
    "@types/ioredis": "^5.0.0",
    "@types/passport-jwt": "^4.0.1",
    "@types/uuid": "^11.0.0",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "axios": "^1.12.2",
    "bullmq": "^5.58.9",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dotenv": "^17.2.2",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.0",
    "jwks-rsa": "^3.2.0",
    "nestjs-cls": "^6.0.1",
    "nestjs-pino": "^4.4.1",
    "node-fetch": "^3.3.2",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "pino-elasticsearch": "^8.1.0",
    "pino-loki": "^2.6.0",
    "pino-pretty": "^13.1.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "typeorm": "^0.3.27",
    "uuid": "^13.0.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.36.0",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.7",
    "@nestjs/testing": "^11.1.6",
    "@swc/cli": "^0.7.8",
    "@swc/core": "^1.13.20",
    "@types/ajv": "^1.0.4",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.2",
    "@types/pg": "^8.15.5",
    "@types/supertest": "^6.0.3",
    "chalk": "^5.6.2",
    "commander": "^14.0.1",
    "diff": "^8.0.2",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "fast-json-stable-stringify": "^2.1.0",
    "globals": "^16.4.0",
    "globby": "^14.1.0",
    "handlebars": "^4.7.8",
    "jest": "^30.1.3",
    "prettier": "^3.6.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.4",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1"
    }
  }
}
